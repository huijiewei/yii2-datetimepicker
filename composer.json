{
  "name": "huijiewei/yii2-datetimepicker",
  "description": "As 'bootstrap-datepicker' is restricted to the date scope (day, month, year), this project aims to support too the time picking (hour, minutes).",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "yii2-extension",
  "homepage": "https://github.com/huijiewei/yii2-datetimepicker.git",
  "keywords": [
    "yii2",
    "extension",
    "dateTimePicker",
    "assets"
  ],
  "authors": [
    {
      "name": "Huijie Wei",
      "email": "huijiewei@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "yiisoft/yii2": "^2.0",
    "huijiewei/yii2-fontawesome": "^2.0",
    "npm-asset/bootstrap-datetime-picker": "^2.4.4"
  },
  "autoload": {
    "psr-4": {
      "huijiewei\\datetimepicker\\": "src"
    }
  },
  "repositories": {
    "asset": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  }
}